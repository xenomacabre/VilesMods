<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<ThingDef ParentName="ApparelNeoliticBase">
		<defName>CE_Apparel_TribalBackpack</defName>
		<label>Pack Basket</label>
		<description>A wicker basket worn on the back, often used by gatherers.</description>
		<!--<thingClass>CombatExtended.Apparel_VisibleAccessory</thingClass>-->
		<graphicData>
			<texPath>Things/Apparel/VilePart2_Primitive/PackBasket/PackBasket</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToMake>3000</WorkToMake>
			<Mass>1</Mass>
			<Bulk>3</Bulk>
			<EquipDelay>2</EquipDelay>			
			<StuffEffectMultiplierInsulation_Cold>0.25</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.25</StuffEffectMultiplierInsulation_Heat>
			<StuffEffectMultiplierArmor>0.26</StuffEffectMultiplierArmor>
			<ArmorRating_Sharp>0</ArmorRating_Sharp>
			<ArmorRating_Blunt>0</ArmorRating_Blunt>
			<ArmorRating_Heat>0</ArmorRating_Heat>
		</statBases>
		<equippedStatOffsets>
			<CarryBulk>14</CarryBulk>
			<MoveSpeed>-0.17</MoveSpeed>
			<WorkSpeedGlobal>-0.15</WorkSpeedGlobal>
			<GermResistance>0.01</GermResistance>
			<GermContainment>0.01</GermContainment>
		</equippedStatOffsets>
		<stuffCategories>
			<li>Leathery</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<costList>
			<Kindling>30</Kindling>
		</costList>
		<generateCommonality>1</generateCommonality>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<careIfWornByCorpse>false</careIfWornByCorpse>
			<bodyPartGroups>
				<li>Shoulders</li>
			</bodyPartGroups>
			<layers>
				<li>Backpack</li>
			</layers>
			<tags>
				<li>Tribal</li>
				<li>Neolithic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
			<wornGraphicPath>Things/Apparel/VilePart2_Primitive/PackBasket/PackBasket</wornGraphicPath>
			<useWornGraphicMask>True</useWornGraphicMask>
			<wornGraphicData>      
				<renderUtilityAsPack>true</renderUtilityAsPack>
				<east>
					<offset>(0,-0.05)</offset>
					<thin>
						<offset>(0.2,0)</offset>
					</thin>
					<hulk>
						<offset>(0.05,0)</offset>
					</hulk>
					<fat>
						<offset>(0,0)</offset>
					</fat>
					<male>
						<offset>(0.2,0)</offset>
					</male>
					<female>
						<offset>(0.2,0)</offset>
					</female>	
				</east>
				<west>
					<offset>(0,-0.05)</offset>
					<thin>
						<offset>(-0.2,0)</offset>
					</thin>
					<hulk>
						<offset>(-0.05,0)</offset>
					</hulk>
					<fat>
						<offset>(0,0)</offset>
					</fat>
					<male>
						<offset>(-0.2,0)</offset>
					</male>
					<female>
						<offset>(-0.2,0)</offset>
					</female>		  
				</west>
				<north>
					<offset>(0,-0.1)</offset> 
					<thin>
						<offset>(0,0)</offset>
					</thin>
					<hulk>
						<offset>(0,-0.05)</offset>
					</hulk>
					<fat>
						<offset>(0,-0.05)</offset>
					</fat>
				</north>		
				<male>
					<scale>(1,1)</scale>
				</male>
				<female>
					<scale>(1,1)</scale>
				</female>
				<thin>
					<scale>(1,1)</scale>
				</thin>
				<hulk>
					<scale>(1,1)</scale>
				</hulk>
				<fat>
					<scale>(1,1)</scale>
				</fat>
			</wornGraphicData>   
		</apparel>
		<thingCategories>
			<li>SpecialApparelCat</li>
		</thingCategories>
		<generateAllowChance>0.8</generateAllowChance>
        <recipeMaker>
            <researchPrerequisite>Apparel_A2</researchPrerequisite>
        </recipeMaker>
	</ThingDef>
	
