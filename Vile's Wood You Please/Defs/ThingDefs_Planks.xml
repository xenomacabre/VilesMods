	<ThingDef ParentName="SK_ResourceBase">
		<defName>ChipBoard</defName>
		<label>chipboard</label>
		<description>Wood sheets and planks fashioned from compressed woodchips. Cheap, ugly, and weak.</description>
		<graphicData>
			<texPath>Things/VileWood/OSB</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
			<color>(255,214,189)</color>
		</graphicData>
		<soundInteract>Wood_Drop</soundInteract>
		<soundDrop>Wood_Drop</soundDrop>
		<stackLimit>300</stackLimit>
		<tickerType>Rare</tickerType>
		<statBases>
			<MarketValue>0.9</MarketValue>
			<Flammability>5.0</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<StuffPower_Armor_Blunt>0.7</StuffPower_Armor_Blunt>
			<StuffPower_Armor_Sharp>0.6</StuffPower_Armor_Sharp>
			<StuffPower_Armor_Heat>0.05</StuffPower_Armor_Heat>
			<StuffPower_Insulation_Cold>1.0</StuffPower_Insulation_Cold>
			<StuffPower_Insulation_Heat>0.5</StuffPower_Insulation_Heat>
			<SharpDamageMultiplier>0.25</SharpDamageMultiplier>
			<BluntDamageMultiplier>0.8</BluntDamageMultiplier>
			<MaxBurningTempCelsius>500</MaxBurningTempCelsius>
			<BurnDurationHours>0.75</BurnDurationHours>
			<Mass>0.3</Mass>
			<Bulk>0.3</Bulk>
		</statBases>
		<techLevel>Neolithic</techLevel>
		<thingCategories>
			<li>EngineeredLumber</li>
		</thingCategories>
		<stuffProps>
			<categories>
				<li>Woody</li>
				<li>EngineeredLumber</li>
			</categories>
			<color>(255,214,189)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpact_Wood</soundImpactStuff>
			<stuffAdjective>chipboard</stuffAdjective>
			<statOffsets>
				<Beauty>-2</Beauty>
			</statOffsets>
			<statFactors>
				<MaxHitPoints>0.65</MaxHitPoints>
				<Flammability>1.3</Flammability>
				<Beauty>0.6</Beauty>
				<WorkToMake>1.1</WorkToMake>
				<WorkToBuild>1.1</WorkToBuild>
				<MeleeWeapon_CooldownMultiplier>1.1</MeleeWeapon_CooldownMultiplier>
				<MoveSpeed>1</MoveSpeed>
				<MarketValue>0.5</MarketValue>
				<BedRestEffectiveness>0.9</BedRestEffectiveness>
				<ImmunityGainSpeedFactor>0.9</ImmunityGainSpeedFactor>
				<WorkTableWorkSpeedFactor>0.5</WorkTableWorkSpeedFactor>
				<DoorOpenSpeed>1.35</DoorOpenSpeed>
				<MeleePenetrationFactor>0.3</MeleePenetrationFactor>
				<Mass>0.6</Mass>
			</statFactors>
		</stuffProps>
		<terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
	</ThingDef>
