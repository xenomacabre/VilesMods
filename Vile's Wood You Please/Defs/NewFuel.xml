<ThingDef ParentName="SK_ResourceBase" Name="FirewoodBase" Abstract="True">
	<description>Wood logs cut into large, rough pieces of firewood. Hardwoods tend to burn longer than softwoods.</description>
	<graphicData>
		<texPath>Things/VileWood/GreenFirewood</texPath>
		<graphicClass>Graphic_StackCount</graphicClass>
	</graphicData>
	<soundInteract>Wood_Drop</soundInteract>
	<soundDrop>Wood_Drop</soundDrop>
	<tickerType>Rare</tickerType>
	<statBases>
		<MarketValue>1.4</MarketValue>
		<MaxBurningTempCelsius>450</MaxBurningTempCelsius>
		<MaxHitPoints>150</MaxHitPoints>
		<Flammability>5.0</Flammability>
		<DeteriorationRate>0</DeteriorationRate>
		<Mass>0.4</Mass>
		<Bulk>0.3</Bulk>
	</statBases>
	<techLevel>Neolithic</techLevel>
	<thingCategories>
		<li>FirewoodGreen</li>
	</thingCategories>
	<recipeMaker>
		<productCount>40</productCount>
		<!--<workAmount>180</workAmount>-->
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<effectWorking>MakeWoodPlanks_Hand</effectWorking>
		<soundWorking>Recipe_MakeWoodPlanks_Hand</soundWorking>
		<recipeUsers>
			<li>ChoppingBlock</li>
			<li>LogSplittingSpot</li>
		</recipeUsers>
    </recipeMaker>
</ThingDef>



