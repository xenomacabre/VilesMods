	<!-- NOMEX -->
	<RecipeDef>
		<defName>MakeNomex</defName>
		<label>Make Nomex x10</label>
		<description>Nomex is a meta-aramid textile with incredible insulation and resistance to heat. Uses Co or Mn as a catalyst and the material is pushed through rollers rather than spun into thread.</description>
		<jobString>Making Nomex.</jobString>
		<workAmount>1500</workAmount>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Sulphates</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Xylene</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Manganese</li>
						<li>Cobalt</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Sulphates</li>
				<li>Xylene</li>	
				<li>Manganese</li>		
				<li>Cobalt</li>		
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Nomex>10</Nomex>
		</products>
		<skillRequirements>
			<Crafting>13</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>AdvancedFabrication</researchPrerequisite>
	</RecipeDef>
