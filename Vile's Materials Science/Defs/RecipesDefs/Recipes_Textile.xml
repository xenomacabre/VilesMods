<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<!-- At loom, near petrochem lab -->
	<RecipeDef>
		<defName>MakePolymerFibers</defName>
		<label>Create polymer fibers x8</label>
		<description>PET plastic is synthesized and the spun into a polyester thread. Useful for polyester fabric, as well as many other recipes.</description>
		<jobString>Making polymer fibers.</jobString>
		<workAmount>1200</workAmount>
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Polymers</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Xylene</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<products>
			<SyntheticFibers>5</SyntheticFibers>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Chemfuel</li>
				<li>Xylene</li>
			</thingDefs>
		</fixedIngredientFilter>
		<skillRequirements>
			<Crafting>12</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Oil_Industry_C6</researchPrerequisite>
	</RecipeDef>
	<!-- Polyester-->
	<RecipeDef>
		<defName>MakePolyester</defName>
		<label>Make Polyester x20</label>
		<description>Polyester is an economy fabric which is easy to sew. It's durable and has good antimicrobial qualities.</description>
		<jobString>Making polyester fabric.</jobString>
		<workAmount>800</workAmount>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<effectWorking>Tailor</effectWorking>
		<soundWorking>Recipe_Tailor</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SyntheticFibers</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SyntheticFibers</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Synthread>20</Synthread>
		</products>
		<skillRequirements>
			<Crafting>12</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>AdvancedFabrication</researchPrerequisite>
	</RecipeDef>
	
