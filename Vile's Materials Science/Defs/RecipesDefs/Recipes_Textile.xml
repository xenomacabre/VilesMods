	<!-- Micropel -->
	<RecipeDef>
		<defName>MakeMicropel</defName>
		<label>Make Micropel x10</label>
		<description>Micropel is a cotton fabric that is treated with carbon nanontubes and silver nanoparticles to add durability and give it unique self-cleaning water-repellant qualities. Especially useful for hospitals.</description>
		<jobString>Making Micropel cloth.</jobString>
		<workAmount>1500</workAmount>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<effectWorking>Smith</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>10</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Carbon</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SilverBar</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Cloth</li>
				<li>Carbon</li>	
				<li>SilverBar</li>	
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Micropel>10</Micropel>
		</products>
		<skillRequirements>
			<Crafting>13</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
		<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Apparel_D1</researchPrerequisite>
	</RecipeDef>
	
		<!-- Dyneema -->
	<RecipeDef>
		<defName>MakeDyneema</defName>
		<label>Make Dyneema x10</label>
