	</ThingDef>

    <ThingDef ParentName="WorkTable">
		<defName>ChemistryLab</defName>
		<label>Chemistry Station</label>
		<description>A table and all the tools needed for mixing and manipulated chemicals. If connected to an electric loom, advanced textiles can be made here. Constructed of corrosion-resistant materials.</description>
        <drawerType>MapMeshAndRealTime</drawerType>
		<graphicData>
			<texPath>Things/Building/ChemStation/Lab</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3,2)</drawSize>
			<shadowData>
				<volume>(2.8, 1.0, 0.85)</volume>
				<offset>(0,0,0)</offset>
			</shadowData>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<size>(3,1)</size>
		<stuffCategories>
			<li>CorrosionResistant</li>
		</stuffCategories>
		<costStuffCount>120</costStuffCount>
        <costList>
			<Glass>80</Glass>
            <ComponentIndustrial>8</ComponentIndustrial>
		</costList>
		<minifiedDef>MinifiedThing</minifiedDef>
		<thingCategories>
			<li>BuildingsProduction</li>
		</thingCategories>
		<statBases>
			<WorkToBuild>2800</WorkToBuild>
			<MaxHitPoints>180</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Cleanliness>-12</Cleanliness>
			<Mass>300</Mass>
		</statBases>
        <recipes>
            <li>MakeChlorine</li>
            <li>MakeCelluloseCotton</li>
            <li>MakeCelluloseWood</li>
            <li>VulcanizeRubber</li>
            <li>MakePTFE</li>
			<li>ExtractSulfur</li>
			<li>MakeAmmonia</li>
        </recipes>
		<designationCategory>Production</designationCategory>
		<interactionCellOffset>(0,0,-1)</interactionCellOffset>
		<tickerType>Normal</tickerType>
		<comps>
			<li>
				<compClass>CompReportWorkSpeed</compClass>
			</li>

            <li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					    <li>Patterner</li>
					<li>TableLoom</li>
				</linkableFacilities>
			</li>

			<li Class="CF.CompProperties_UnlocksRecipe">
			   <linkableFacilities>
				  <li>
					 <targetFacility>TableLoom</targetFacility>
				  </li>
			   </linkableFacilities>
			    <recipes>
                    <li>MakePolymerFibers</li>
                    <li>MakePolyester</li>
                    <li>MakeRayonViscose</li>
                    <li>MakeMycotanLeather</li>
                    <li>MakePVCLeather</li>
                    <li>MakeNomex</li>
                    <li>MakeKevlar</li>
                    <li>MakeMicropel</li>
                    <li>MakeDyneema</li>
                    <li>MakeZylon</li>
				 </recipes>
            </li>

		</comps>
		<building>
			<uninstallWork>1450</uninstallWork>
		</building>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
			<li>PlaceWorker_ReportWorkSpeedPenalties</li>
		</placeWorkers>
		<constructionSkillPrerequisite>5</constructionSkillPrerequisite>
        <researchPrerequisites>
			<li>Chemistry_C0</li>
		</researchPrerequisites>
	</ThingDef>

</Defs>
