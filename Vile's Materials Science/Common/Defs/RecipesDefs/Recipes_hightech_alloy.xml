<Defs>
	<RecipeDef>
		<defName>MakeMaxametSteel</defName>
		<label>Make Maxamet high-speed steel x25</label>
		<description>Maxamet is a brand of high-speed tool steel with excellent edge retention. It fairs well in most applications, but it is unsurpased in bladed tools and weapons.</description>
		<jobString>Making Maxamet steel</jobString>
		<workAmount>2100</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>PigIron</li>
					</thingDefs>
				</filter>
				<count>12</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Cobalt</li>
					</thingDefs>
				</filter>
				<count>4</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Tungsten</li>
					</thingDefs>
				</filter>
				<count>6</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Vanadium</li>
					</thingDefs>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
						<li>PigIron</li>
						<li>Cobalt</li>
						<li>Tungsten</li>
						<li>Vanadium</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<MaxametSteel>25</MaxametSteel>
		</products>
		<skillRequirements>
			<Crafting>15</Crafting>
		</skillRequirements>
		<workSkill>Crafting</workSkill>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
		<researchPrerequisite>Metals_D1</researchPrerequisite>
	</RecipeDef>
	
