<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<RecipeDef Class="UniversalFermenterSK.RecipeDef_UF">
		<defName>RackDry_Rawhide</defName>
		<label>make rawhide</label>
		<description>Rawhide is made by curing and treating raw skins and hides, but it's not tanned. It dries very hard and can be used in place of metal in some armor recipes.</description>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>CuredHides</li>
						<li>UncuredHides</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>CuredHides</li>
				<li>UncuredHides</li>
			</categories>
		</fixedIngredientFilter>
		<products>
			<Rawhide>10</Rawhide>
		</products>
		<recipeUsers>
			<li>TanningRack</li>
		</recipeUsers>
		<temperatureSafe>
			<min>0</min>
			<max>30</max>
		</temperatureSafe>
		<temperatureIdeal>
			<min>5</min>
			<max>25</max>
		</temperatureIdeal>
		<ruinedPerDegreePerHour>0</ruinedPerDegreePerHour>
		<processDays>1</processDays>
	</RecipeDef>
	
