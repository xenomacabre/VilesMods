<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- SALTING -->
	<RecipeDef>
		<defName>BrineCure_ExoticSkins</defName>
		<label>cure exotic skins</label>
		<jobString>Brine-curing.</jobString>
		<description>Skins and hides are submersed in salty water. The water must be paddled by hand, though it's much faster than dry curing.</description>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Hide_ExoticSkin</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Hide_ExoticSkin</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Hide_ExoticSkinCured>1</Hide_ExoticSkinCured>
		</products>
		<recipeUsers>
			<li>BriningStation</li>
		</recipeUsers>
		<workSpeedStat>TailoringSpeed</workSpeedStat>
		<workSkill>Crafting</workSkill>
		<workAmount>150</workAmount>
	</RecipeDef>
