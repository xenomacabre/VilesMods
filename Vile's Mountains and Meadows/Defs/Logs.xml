		<graphicData>
			<color>(194, 142, 74)</color>
		</graphicData>
		<stuffProps>
			<stuffAdjective>stone pine log</stuffAdjective>
			<color>(194, 142, 74)</color>
		</stuffProps>
		<butcherProducts>
			<Pitch>40</Pitch>
			<TreeBark MayRequire="vitech.leathertanning">20</TreeBark>
			<Kindling>15</Kindling>
			<Seed_StonePine>1</Seed_StonePine>
		</butcherProducts>
	</ThingDef>	

