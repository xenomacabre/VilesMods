<Defs>

<ThingDef Name="HolmOakLog" ParentName="OakLog">
		<defName>HolmOakLog</defName>
		<label>holm oak log</label>
		<description>The saying "Strong as an oak" also pertains to the lumber. This variety of oak from the holm oak tree.</description>
		<graphicData>
			<color>(138,114,76)</color>
		</graphicData>
		<stuffProps>
			<stuffAdjective>holm oak log</stuffAdjective>
			<color>(184, 126, 18)</color>
		</stuffProps>
		<butcherProducts>
			<TreeBark MayRequire="vitech.leathertanning">35</TreeBark>
			<Kindling>15</Kindling>
			<Seed_TreeHolmOak>1</Seed_TreeHolmOak>
		</butcherProducts>
	</ThingDef>	
<ThingDef Name="UmbrellaPineLog" ParentName="PineLog">
