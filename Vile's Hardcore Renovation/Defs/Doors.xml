	<!--========================= Farm Gate =============================-->
	<ThingDef ParentName="SK_DoorBase">
		<defName>FenceGate</defName>
		<label>farm fence gate</label>
		<description>A gate made of planks. Opens much quicker than wattle fences.</description>
		<statBases>
			<MaxHitPoints>180</MaxHitPoints>
			<WorkToBuild>250</WorkToBuild>
			<DoorOpenSpeed>1.5</DoorOpenSpeed>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/Multi/WoodFenceGate_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shadowData>
				<volume>(0.3, 0.5, 0.3)</volume>
			</shadowData>
			<linkFlags>
				<li>Custom1</li>
			</linkFlags>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/Multi/WoodFenceGateIco_south</uiIconPath>
		<stuffCategories>
			<li>SoftwoodLumber</li>
			<li>HardwoodLumber</li>
			<li>UltrahardwoodLumber</li>
			<li>EngineeredLumber</li>
		</stuffCategories>
		<costStuffCount>5</costStuffCount>
		<costList>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<holdsRoof>false</holdsRoof>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<blockWind>false</blockWind>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<relatedBuildCommands>
				<li>Fence</li>
				<li>PenMarker</li>
			</relatedBuildCommands>
			<blueprintGraphicData>
				<texPath>Things/Building/Doors/Multi/WoodFenceGateIco</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
			</blueprintGraphicData>
			<soundDoorOpenManual>Door_FenceGateManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_FenceGateManual</soundDoorCloseManual>
		</building>
		<comps>
			<li Class="CompProperties_Forbiddable" />
		</comps>
	</ThingDef>
	
</Defs>