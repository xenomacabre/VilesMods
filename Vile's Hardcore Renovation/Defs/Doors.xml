<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<ThingDef ParentName="SK_DoorBase">
		<defName>DoorBarn</defName>
		<label>top-hung door</label>
		<description>A simple sliding door with a barn aesthetic. Heavier than a normal door and opens slower.</description>
		<statBases>
			<MaxHitPoints>220</MaxHitPoints>
			<WorkToBuild>600</WorkToBuild>
			<DoorOpenSpeed>1</DoorOpenSpeed>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/DoorBarn_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/DoorBarn_MenuIcon</uiIconPath>
		<stuffCategories>
			<li>SoftwoodLumber</li>
			<li>HardwoodLumber</li>
			<li>UltrahardwoodLumber</li>
			<li>EngineeredLumber</li>
		</stuffCategories>
		<costStuffCount>20</costStuffCount>
		<!--<costList>
			<ComponentMedieval>1</ComponentMedieval>
		</costList>-->
		<!--<building>
		  <roamerCanOpen>true</roamerCanOpen>
		</building>-->
	</ThingDef>

	<ThingDef ParentName="SK_DoorBase">
		<defName>DoorOrnate</defName>
		<label>ornate door</label>
		<description>A door with ornate hinges and reinforced with metal bands.</description>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToBuild>1200</WorkToBuild>
			<Beauty>6</Beauty>
			<DoorOpenSpeed>0.85</DoorOpenSpeed>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/DoorOrnate_Mover</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/DoorOrnate_MenuIcon</uiIconPath>
		<stuffCategories>
			<li>SoftwoodLumber</li>
			<li>HardwoodLumber</li>
			<li>UltrahardwoodLumber</li>
			<li>EngineeredLumber</li>
		</stuffCategories>
		<costStuffCount>30</costStuffCount>
		<costList>
			<ComponentMedieval>5</ComponentMedieval>
		</costList>
	</ThingDef>
	<ThingDef ParentName="SK_DoorBase">
		<defName>PalisadeGate</defName>
		<label>palisade fence gate</label>
		<description>A strong gate made of wooden logs.</description>
		<statBases>
			<MaxHitPoints>300</MaxHitPoints>
			<WorkToBuild>500</WorkToBuild>
			<DoorOpenSpeed>0.75</DoorOpenSpeed>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/Multi/GatePalisade_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shadowData>
				<volume>(0.3, 0.5, 0.3)</volume>
			</shadowData>
			<linkFlags>
				<li>Custom1</li>
			</linkFlags>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/Multi/GatePalisadeIco_south</uiIconPath>
		<stuffCategories>
			<li>Softwoods</li>
			<li>Hardwoods</li>
			<li>Ultrahardwoods</li>
		</stuffCategories>
		<costStuffCount>3</costStuffCount>
		<costList>
			<ComponentMedieval>4</ComponentMedieval>
		</costList>
		<holdsRoof>false</holdsRoof>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<blockWind>false</blockWind>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<relatedBuildCommands>
				<li>Fence</li>
				<li>PenMarker</li>
			</relatedBuildCommands>
			<blueprintGraphicData>
				<texPath>Things/Building/Doors/Multi/GatePalisadeIco</texPath>
				<graphicClass>Graphic_Multi</graphicClass>
			</blueprintGraphicData>
			<soundDoorOpenManual>Door_FenceGateManual</soundDoorOpenManual>
			<soundDoorCloseManual>Door_FenceGateManual</soundDoorCloseManual>
		</building>
		<comps>
			<li Class="CompProperties_Forbiddable" />
		</comps>
	</ThingDef>
