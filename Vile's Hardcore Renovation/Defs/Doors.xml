	<ThingDef ParentName="SK_DoorBase">
		<defName>WattleFenceGate</defName>
		<label>wattle fence gate</label>
		<description>A gate made of sticks.</description>
		<statBases>
			<MaxHitPoints>40</MaxHitPoints>
			<WorkToBuild>200</WorkToBuild>
			<DoorOpenSpeed>1</DoorOpenSpeed>
		</statBases>
		<graphicData>
			<texPath>Things/Building/Doors/Multi/WattleFenceGate_Mover</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shadowData>
				<volume>(0.3, 0.5, 0.3)</volume>
			</shadowData>
			<linkFlags>
				<li>Custom1</li>
			</linkFlags>
			<damageData>
				<enabled>false</enabled>
			</damageData>
		</graphicData>
		<uiIconPath>Things/Building/Doors/Multi/WattleGateIco_south</uiIconPath>
		<stuffCategories>
			<li>WoodLogs</li>
		</stuffCategories>
		<costStuffCount>5</costStuffCount>
		<costList>
			<Kindling>4</Kindling>
		</costList>
		<holdsRoof>false</holdsRoof>
		<staticSunShadowHeight>0</staticSunShadowHeight>
		<blockWind>false</blockWind>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
			<relatedBuildCommands>
				<li>Fence</li>
				<li>PenMarker</li>
			</relatedBuildCommands>
